#container {
    flex-direction: row;
    justify-content: space-around;
    height: 16px;
    align-items: center;
    -unity-text-align: middle-left;
}

#content {
    flex-grow: 1;
    flex-direction: row;
}

#content #title {
    width: 40px;
}

#content #value-field {
    flex-grow: 1;
    flex-direction: row;
}

#value-field > #field {
    flex-grow: 1;
}
